<mapper namespace="kr.or.ddit.messenger.dao.MessengerDAO">

	<resultMap type="ChatRoomVO" id="ChatRoomMap" autoMapping="true">
		<id property="chatRoomCd" column="CHAT_ROOM_CD"/>
		<collection property="partiesList" ofType="ChatParticipantVO" autoMapping="true"/>
	</resultMap>

	<insert id="insertChat" parameterType="ChatRoomVO">
		INSERT INTO chat_room (
		    chat_room_cd,
		    chat_room_yn
		) VALUES (
		    #{chatRoomCd,jdbcType=VARCHAR}
		    , #{chatRoomYn,jdbcType=VARCHAR}
		)
	</insert>
</mapper>
