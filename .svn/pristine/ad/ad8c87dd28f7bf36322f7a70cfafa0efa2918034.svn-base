<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core"  prefix="c" %>

<table>
	<thead>
		<tr>
			<th>프로젝트명</th>
			<th>생성일</th>
		</tr>
	</thead>
	
	<c:if test="${not empty projectList }">
		<c:forEach items="projectList" var="project">
			<tr>
				<td>${project.proNm }</td>
				<td>${project.proBdate }</td>
			</tr>
		</c:forEach>
	</c:if>
</table>