<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.groupware.webhard.dao.WebHardDAO">
	<insert id="insertFile" parameterType="kr.or.ddit.vo.groupware.WebHardVO">
		INSERT INTO WEBHARD(WEB_SNM, WEB_COURS, WEB_TY, WEB_RNM)
		VALUES(#{webSnm},
			#{webCours},
			#{webTy},
			#{webRnm})
	</insert>
	<select id="selectListFile">
		SELECT 123
		FROM DUAL
	</select>
</mapper>
